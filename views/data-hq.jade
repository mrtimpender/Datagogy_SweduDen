doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    title DataGogy
    // Normalize
    link(rel='stylesheet', href='https://cdnjs.cloudflare.com/ajax/libs/normalize/4.1.1/normalize.min.css')
    // Bootstrap
    link(rel='stylesheet', href='https://cdnjs.cloudflare.com/ajax/libs/bootswatch/3.3.6/spacelab/bootstrap.min.css')
    link(href='https://fonts.googleapis.com/css?family=Playfair+Display', rel='stylesheet', type='text/css')
    link(rel='stylesheet', type='text/css', href='style-data-hq.css')
  body
    // Navigation Bar
    nav.navbar.navbar-default
      .col-xs-8
        a.btn.btn-success.navbar-btn(href='/dashboard') Dashboard
        a.btn.btn-success.navbar-btn(href='/create-quiz') Quiz Creation
        a.btn.btn-success.navbar-btn(href='/generate-report') Report Center
        a.btn.btn-success.navbar-btn(href='/data-hq') Data HQ
      .col-xs-4.text-right
        a.btn.btn-warning.navbar-btn(href='/logout') Log out
    .container-fluid.col-lg-12
      .row
        .col-lg-4
          h1.header Data Headquarters
          p.gap.header Information at your fingertips
    .container-fluid.col-lg-12.gap.filter
      .row
        h3.subheader Select Your Filters
        a.gap.btn.btn-info.navbar-btn(href='#') Question
        a.gap.btn.btn-info.navbar-btn(href='#') School
        a.gap.btn.btn-info.navbar-btn(href='#') District
        a.gap.btn.btn-info.navbar-btn(href='#') Grade Level
        a.gap.btn.btn-info.navbar-btn(href='#') Class
    .row
      .col-lg-3.super-gap
        img(src='bubble.png')
      .col-lg-4
        img(src='growth.png')
      .col-lg-4
        img(src='histogram.png')
    script(src='https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js')
    script(src='https://use.fontawesome.com/f4b058bfe8.js')
    script(type='text/javascript', src='js/quiz.js')
